---
title: "what they wrote"
layout: "layouts/base.njk"
pagination:
  data: collections.news
  addAllPagesToCollections: true
  size: 10
  reverse: true
permalink: /news/{% if pagination.pageNumber > 0 %}page/{{ pagination.pageNumber }}/{% endif %}index.html
---

<ul class="news">
  {% for post in pagination.items %}
    <div class="newsbox">
      <li>
      {%include  'components/newspost.njk'%}
      </li>
    </div>
  {% endfor %}
</ul>

<div class="news-navigation paging">
  {%include 'components/paging.njk'%}
</div>